# Configuración del entorno
ENVIRONMENT=local

# Configuración del servidor
HOST=0.0.0.0
PORT=3000
DEBUG=true

# Logging
LOG_LEVEL=DEBUG

# CORS - Lista de orígenes permitidos separados por coma
